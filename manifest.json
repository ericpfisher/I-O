{
	"manifest_version": 2,
	"name": "Testing",
	"version": "1.0",

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},

	"page_action": {
		"default_icon": "zero19.png",
		"default_popup": "popup.html"
	},

	"content_scripts": [
		{
			"matches": ["https://prod-servicedesk.publicisgroupe.net/*"],
			"js": ["content.js"]
		}
	],

	"permissions": [
		"tabs",
		"storage",
		"http://10.3.35.30/lco/internal/"
	]
}